{
  "_version": "1.65.0",
  "sap.app": {
    "id": "project1",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.18.1",
      "toolsId": "c5b3fb6b-0b9c-4dcf-8703-41d5a27a6c31"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.136.2",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "project1.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "project1.view",
        "async": true,
        "viewPath": "project1.view"
      },
      "routes": [
        {
          "name": "RouteView1",
          "pattern": ":?query:",
          "target": [
            "TargetView"
          ]
        },
        {
          "name": "dashboard",
          "pattern": "dashboard",
          "target": [
            "Dashboard"
          ]
        },
        {
          "name": "employee",
          "pattern": "employee",
          "target": [
            "Employee"
          ]
        },
        {
          "name": "employee_detail",
          "pattern": "employee_detail/{id}/{layout}",
          "target": [
            "Employee",
            "EmployeeDetail"
          ]
        },
        {
          "name": "create_employee",
          "pattern": "create_employee/{layout}",
          "target": [
            "Employee",
            "CreateEmployee"
          ]
        },
        {
          "name": "user",
          "pattern": "user",
          "target": [
            "User"
          ]
        },
        {
          "name": "userdetail",
          "pattern": "userdetail/{id}/{layout}",
          "target": [
            "User",
            "UserDetail"
          ]
        },
        {
          "name": "createUser",
          "pattern": "createUser/{layout}",
          "target": [
            "User",
            "CreateUser"
          ]
        },
        {
          "name": "loanapplication",
          "pattern": "loanapplication",
          "target": [
            "LoanApplication"
          ]
        },
        {
          "name": "createLoanApplication",
          "pattern": "createLoanApplication/{layout}",
          "target": [
            "LoanApplication",
            "CreateLoanApplication"
          ]
        },
        {
          "name": "lettersandcertificates",
          "pattern": "lettersandcertificates",
          "target": [
            "LettersAndCertificates"
          ]
        },
        {
          "name": "createlettersandcertificates",
          "pattern": "createlettersandcertificates/{layout}",
          "target": [
            "LettersAndCertificates",
            "CreateLettersAndCertificates"
          ]
        },
        {
          "name": "mylettersandcertificates",
          "pattern": "mylettersandcertificates",
          "target": [
            "MyLettersandCertificates"
          ]
        },
        {
          "name": "createMylettersandcertificates",
          "pattern": "createMylettersandcertificates/{layout}",
          "target": [
            "MyLettersandCertificates",
            "CreateLettersAndCertificates"
          ]
        },
        {
          "name": "teamlettersandcertificates",
          "pattern": "teamlettersandcertificates",
          "target": [
            "TeamLettersandCertificates"
          ]
        },
        {
          "name": "createteamlettersandcertificates",
          "pattern": "createteamlettersandcertificates/{layout}",
          "target": [
            "TeamLettersandCertificates",
            "CreateTeamLettersAndCertificates"
          ]
        }
      ],
      "targets": {
        "TargetView": {
          "id": "View",
          "name": "View"
        },
        "Dashboard": {
          "viewType": "XML",
          "transition": "slide",
          "viewName": "Dashboard"
        },
        "Employee": {
          "viewPath": "project1.employee",
          "viewName": "Employee",
          "viewType": "XML",
          "transition": "slide",
          "viewId": "Employee",
          "parent": "AppUnified"
        },
        "EmployeeDetail": {
          "viewPath": "project1.employee",
          "viewName": "EmployeeDetail",
          "viewType": "XML",
          "transition": "slide",
          "viewId": "EmployeeDetail",
          "parent": "AppUnified"
        },
        "CreateEmployee": {
          "viewPath": "project1.employee",
          "viewName": "CreateEmployee",
          "viewType": "XML",
          "transition": "slide",
          "viewId": "CreateEmployee",
          "parent": "AppUnified"
        },
        "User": {
          "viewPath": "project1.user",
          "viewName": "User",
          "viewType": "XML",
          "transition": "slide",
          "viewId": "User",
          "parent": "AppUnified"
        },
        "UserDetail": {
          "viewPath": "project1.user",
          "viewName": "UserDetail",
          "viewType": "XML",
          "transition": "slide",
          "viewId": "UserDetail",
          "parent": "AppUnified"
        },
        "CreateUser": {
          "viewPath": "project1.user",
          "viewName": "CreateUser",
          "viewType": "XML",
          "transition": "slide",
          "viewId": "CreateUser",
          "parent": "AppUnified"
        },
        "LoanApplication": {
          "viewPath": "project1.loanapplication",
          "viewName": "LoanApplication",
          "viewType": "XML",
          "transition": "slide",
          "viewId": "LoanApplication",
          "parent": "AppUnified"
        },
        "CreateLoanApplication": {
          "viewPath": "project1.loanapplication",
          "viewName": "CreateLoanApplication",
          "viewType": "XML",
          "transition": "slide",
          "viewId": "CreateLoanApplication",
          "parent": "AppUnified"
        },
        "LettersAndCertificates": {
          "viewPath": "project1.lettersAndcertificates",
          "viewName": "LettersAndCertificates",
          "viewType": "XML",
          "transition": "slide",
          "viewId": "LettersAndCertificates",
          "parent": "AppUnified"
        },
        "CreateLettersAndCertificates": {
          "viewPath": "project1.lettersAndcertificates",
          "viewName": "CreateLettersAndCertificates",
          "viewType": "XML",
          "transition": "slide",
          "viewId": "CreateLettersAndCertificates",
          "parent": "AppUnified"
        },
        "MyLettersandCertificates": {
          "viewPath": "project1.mylettersandcertificates",
          "viewName": "mylettersandcertificates",
          "viewType": "XML",
          "transition": "slide",
          "viewId": "mylettersandcertificates",
          "parent": "AppUnified"
        },
        "CreateMyLettersAndCertificates": {
          "viewPath": "project1.mylettersandcertificates",
          "viewName": "createmylettersandcertificates",
          "viewType": "XML",
          "transition": "slide",
          "viewId": "createmylettersandcertificates",
          "parent": "AppUnified"
        },
        "TeamLettersandCertificates": {
          "viewPath": "project1.teamlettersandcertificates",
          "viewName": "teamlettersandcertificates",
          "viewType": "XML",
          "transition": "slide",
          "viewId": "teamlettersandcertificates",
          "parent": "AppUnified"
        },
        "CreateTeamLettersAndCertificates": {
          "viewPath": "project1.teamLettersAndCertificates",
          "viewName": "createteamlettersandcertificates",
          "viewType": "XML",
          "transition": "slide",
          "viewId": "createteamlettersandcertificates",
          "parent": "AppUnified"
        }
      }
    },
    "rootView": {
      "viewName": "project1.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}