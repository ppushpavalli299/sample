TABLE deductions (
      id BIGINT GENERATED BY DEFAULT AS IDENTITY PRIMARY KEY,
    employee_number VARCHAR(50),
    name VARCHAR(100),
    cost VARCHAR(50),
    FOREIGN KEY (employee_number) REFERENCES payslip(employee_number)
);

TABLE deductions (
    id BIGINT GENERATED BY DEFAULT AS IDENTITY PRIMARY KEY,
    employee_number VARCHAR(50),
    name VARCHAR(100),
    cost VARCHAR(50),
    FOREIGN KEY (employee_number) REFERENCES payslip(employee_number)
);