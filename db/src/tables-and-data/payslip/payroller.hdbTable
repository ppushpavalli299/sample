COLUMN TABLE "db.tables-and-data.payslip::PAYROLLER" (
    id BIGINT GENERATED BY DEFAULT AS IDENTITY PRIMARY KEY,
    employee_number VARCHAR(50),
    total_earnings VARCHAR(50),
    total_deductions VARCHAR(50),
    net_pay VARCHAR(50),
    FOREIGN KEY (employee_number) REFERENCES "db.tables-and-data.payslip::PAYSLIP"(employee_number)
);