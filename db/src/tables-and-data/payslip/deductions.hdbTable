COLUMN TABLE "db.tables-and-data.payslip::DEDUCTIONS" (
    ID BIGINT GENERATED BY DEFAULT AS IDENTITY PRIMARY KEY,
    EMPLOYEE_NUMBER NVARCHAR(50),
    NAME NVARCHAR(100),
    COST NVARCHAR(50),
    FOREIGN KEY (EMPLOYEE_NUMBER) 
        REFERENCES "db.tables-and-data.payslip::PAYSLIP"(EMPLOYEE_NUMBER)
)
